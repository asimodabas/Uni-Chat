<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/page_background"
    tools:context=".ui.fragment.SecondFragment">


    <Button
        android:id="@+id/teacherButton"
        android:layout_width="120dp"
        android:layout_height="48dp"
        android:backgroundTint="@color/black"
        android:text="Öğretmen"
        app:layout_constraintBottom_toTopOf="@+id/button9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/PublicButton"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/engineerButton"
        android:layout_width="120dp"
        android:layout_height="48dp"
        android:backgroundTint="@color/black"
        android:text="Mühendis"
        app:layout_constraintBottom_toTopOf="@+id/healthButton"
        app:layout_constraintEnd_toStartOf="@+id/PublicButton"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/PublicButton"
        android:layout_width="120dp"
        android:layout_height="48dp"
        android:backgroundTint="@color/black"
        android:text="Genel"
        app:layout_constraintBottom_toTopOf="@+id/button8"
        app:layout_constraintEnd_toStartOf="@+id/teacherButton"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/engineerButton"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/healthButton"
        android:layout_width="120dp"
        android:layout_height="48dp"
        android:backgroundTint="@color/black"
        android:text="sağlık"
        app:layout_constraintBottom_toTopOf="@+id/button10"
        app:layout_constraintEnd_toStartOf="@+id/button8"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/engineerButton" />

    <Button
        android:id="@+id/button8"
        android:layout_width="120dp"
        android:layout_height="48dp"
        android:backgroundTint="@color/black"
        android:text="Button"
        app:layout_constraintBottom_toTopOf="@+id/button11"
        app:layout_constraintEnd_toStartOf="@+id/button9"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/healthButton"
        app:layout_constraintTop_toBottomOf="@+id/PublicButton" />

    <Button
        android:id="@+id/button9"
        android:layout_width="120dp"
        android:layout_height="48dp"
        android:backgroundTint="@color/black"
        android:text="Button"
        app:layout_constraintBottom_toTopOf="@+id/button12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button8"
        app:layout_constraintTop_toBottomOf="@+id/teacherButton" />

    <Button
        android:id="@+id/button10"
        android:layout_width="120dp"
        android:layout_height="48dp"
        android:backgroundTint="@color/black"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button11"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/healthButton" />

    <Button
        android:id="@+id/button11"
        android:layout_width="120dp"
        android:layout_height="48dp"
        android:backgroundTint="@color/black"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button12"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button10"
        app:layout_constraintTop_toBottomOf="@+id/button8" />

    <Button
        android:id="@+id/button12"
        android:layout_width="120dp"
        android:layout_height="48dp"
        android:backgroundTint="@color/black"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button11"
        app:layout_constraintTop_toBottomOf="@+id/button9" />
</androidx.constraintlayout.widget.ConstraintLayout>